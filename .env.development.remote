# Remote Development Environment Configuration
# These variables are used for the hosted dev environment (Render)

# Environment Identifier
VITE_ENVIRONMENT=development

# Debug Settings (enabled for dev environment debugging)
VITE_DEBUG_MODE=true

# Feature Flags (useful for testing new features in dev)
VITE_ENABLE_DEBUG_LOGGING=true

# Dev Environment Specific Settings
VITE_DEV_BANNER=true